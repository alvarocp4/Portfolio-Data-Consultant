{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/about/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/author/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/category/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/page/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/tag/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/portfolio/?$",
			"dest": "_render"
		},
		{
			"src": "^/rss/category/([^/]+?)\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/rss/tag/([^/]+?)\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/rss\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}